key_up = keyboard_check(vk_up);
key_down = keyboard_check(vk_down);
key_space = keyboard_check_pressed(vk_space);
//Para desplazar al personaje
h_direction = -key_left + key_right;
v_direction = -key_up + key_down;
hsp = (-key_left + key_right) * 5;
vsp = (-key_up + key_down) * 5;
//Screen limits
if(x + hsp &lt; 16 || x + hsp &gt; room_height -16)
    hsp = 0;
if(y + vsp &lt; 16 || y + vsp &gt; room_width -16)
    vsp = 0;
//Usando x,y
x += hsp;
y += vsp;
//To go to the place
if(key_space = true &amp;&amp; place_to_go != "None"){
    room_goto(place_to_go);
}
//Set show place to false when finish
show_place = false;
place_to_go = "None";
</string>
