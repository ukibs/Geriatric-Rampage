key_up = keyboard_check(vk_up);
key_down = keyboard_check(vk_down);
key_space = keyboard_check_pressed(vk_space);
if(status_talking == false){
//Para desplazar al personaje
    h_direction = -key_left + key_right;
    v_direction = -key_up + key_down;
    hsp = (-key_left + key_right) * 5;
    vsp = (-key_up + key_down) * 5;
    //Usando x,y
    x += hsp;
    y += vsp;
}
//Changing sprite
if(hsp &lt; 0)
    sprite_index = spr_walking_left;
if(hsp &gt; 0)
    sprite_index = spr_walking_right;
if(vsp &lt; 0)
    sprite_index = spr_walking_up;
if(vsp &gt; 0)
    sprite_index = spr_walking_down;
//Get that sprite
    sprite_to_use = sprite_index;
//Get the direction
switch(sprite_to_use){
    case spr_walking_left:
        direction = 180;
        break;
    case spr_walking_right:
        direction = 0;
        break;
    case spr_walking_up:
        direction = 90;
        break;
    case spr_walking_down:
        direction = 270;
        break;
}
</string>
